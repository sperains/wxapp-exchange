page{
  background-color: #f5f5f5;
}

.top-content{
  background-color: #39404d;
}

.navagitorbar {
  height: 88rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  font-size: 32rpx;
  color: #fff;
  justify-content: space-between;
}

.navagitorbar image{
  height: 36rpx;
  width: 36rpx;
  background-size:  cover;
  float: left;
  margin: 15rpx 36rpx;
}


.top-content_detail{
  height: 320rpx;
  display: flex;
  flex-direction: column;
}

.top-content_title{
  height: 88rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  margin-top: 20rpx;
  padding-left: 36rpx;
}

.top-content_title image{
  height: 36rpx;
  width: 36rpx;
  
  margin-right: 10rpx;
}

.top-content_title text{
  font-size: 28rpx;
  color: #fff;
}

.top-content_data {
  margin-top: 30rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  font-size: 72rpx;
  color: #cba064;
}

.top-content_data .first{
  font-size: 24rpx;
  color: #fff;
}

.mid-content{
  display: flex;
  flex-direction: row;
  height: 176rpx;
  background-color: #fff;
}

.mid-content_left{
  height: 120rpx;
  width: 120rpx;
  background-color: #fff;
  margin: 0 34rpx;
  align-self: center;
}

.mid-content_right {
  flex: 1;
  display: flex;
  flex-direction: column;
  width: 100%;
}

.mid-content_right .text{
  flex: 1;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  color: #999;
  border-bottom: solid 1rpx #ddd;
  padding-right: 36rpx;
}

.mid-content_right .text .last{
  color: #39404d;
  font-weight: bold;
}

.mid-content_right .last{
  border-bottom: none;
}

.content-info{
  margin-top: 20rpx;
  background-color: #fff;
  padding-left: 36rpx;
}

.content-info view{
  height: 88rpx;
  border-bottom: solid 1rpx #ddd;
  line-height: 88rpx;
  display: flex ;
  justify-content: space-between;
  padding-right: 36rpx;
  font-size: 28rpx;
  color: #999;
}

.content-info view .last{
  color: #39404d;
}

.content-info view .letter{
  letter-spacing: 10px;
}

.content-info .last{
  border-bottom: none;
}

.record {
  flex: 1;
  display: flex;
  flex-direction: column;
  background-color: #fff;
  margin-top: 20rpx;
}

.record-tab{
  display: flex;
  flex-direction: row;
  justify-content: space-around;
  text-align: center;
  align-items: center;
  height: 88rpx;
}

.record-tab_item{
  width: 33%;
  line-height: 88rpx;
  font-size: 28rpx;
}

.record-content{
  flex: 1;
  display: flex;
  flex-direction: column;
}

.record-content_item{
  height: 88rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 28rpx;
  margin: 0 36rpx;
}


.submit{
  margin-top: 5rpx;
  background-color: #fff;
  display: flex;
  flex-direction: row;
  height: 100rpx;
  align-items: center;
  justify-content: space-between;
  font-size: 30rpx;
  padding-left: 36rpx;
}

.submit view .remark{
  color: #cba064;
  margin: 0 10rpx;
}

.submit-btn{
  width: 280rpx;
  height: 100%;
  background-color: #cba064;
  line-height: 100rpx;
  text-align: center;
  color: #fff;
  font-size: 32rpx;
}


.tenderDetail {
  position: fixed;
  /*top:408rpx;*/
  top:100%;
  height: 100%;
  width: 100%;
  background-color: #f5f5f5;
  display: flex;
  flex-direction: column;
}

.tender-range{
  height: 88rpx;
  line-height: 88rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  background-color: #39404d;
  padding: 0 36rpx;
  color: #fff;
  font-size: 28rpx;
}

.tender-range image{
  height: 36rpx;
  width: 36rpx;
}

.tender-bank{
  height: 120rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  padding: 0 36rpx;
  background-color: #fff;
}

.tender-bank image{
  height: 64rpx;
  width: 64rpx;
}

.tender-bank_left{
  display: flex;
  flex-direction: row;

}

.tender-bank_left image{
  margin-right: 20rpx;
}

.tender-bank .right{
  height: 36rpx;
  width: 36rpx;
}

.tender-bank_text{
  display: flex;
  flex-direction: column;
  font-size: 20rpx;
  color: #999;
}

.tender-bank_text .top{
  font-size: 28rpx;
  color: #1c1f26;
}

.tender-money{
  margin-top: 72rpx;
  width: 100%;
  height: fit-content;
}

.tender-money_input{
  height: 100rpx;
  background-color: #fff;
  margin: 0 36rpx;
  box-sizing: border-box;
  border: solid 1rpx #ddd;
}

.tender-money_input input{
  padding-left: 24rpx;
  height: 100rpx;
  line-height: 100rpx;
  font-size: 28rpx;
  color: #999999;
}

.tender-money_submit{
  margin: 36rpx 36rpx 36rpx 36rpx;
  height: 100rpx;
  background-color: #cba064;
  text-align: center;
  line-height: 100rpx;
  font-size: 32rpx;
  color: #fff;
}

.opacity{ 
  position: fixed;
  height: 100%;
  width: 100%;
  top: 0;
  opacity:0.3; 
  background-color:#000000; 
  transition:#fff 0.4s ease ;
} 