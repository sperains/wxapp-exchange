
.navagitorbar {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  font-size: 25rpx;
  font-weight: bold;
}

.navagitorbar image{
  height: 36rpx;
  width: 36rpx;
  background-size:  cover;
  margin: 15rpx 36rpx;
}

.titleView{
  display: flex;
  height: 336rpx;
  flex-direction: column;
  background-color: #c8a064;
}

.title-name {
  flex: 3 ;
  display: flex;
  flex-direction: column;
}

.title-exchangeMoney {
  flex: 1 ;
}

.cn-title {
  font-size: 36rpx;
  color: #fff;
  font-weight:  bold;
  margin-top: 36rpx;
  padding-left: 36rpx;
}

.en-title {
  font-size: 24rpx;
  color: #fff;
  padding-top: 10rpx;
  padding-left: 36rpx;
}

.title-exchangeMoney{
  display: flex;
  flex-direction: column-reverse;
  font-size: 28rpx;
  padding-left: 36rpx;
  color: #fff;
  margin-bottom: 36rpx;
}

.nav-item {
  height: 150rpx;
  display: flex;
  flex-direction: row;
  border-top: solid 1px #DDDDDD;
  margin-left:36rpx; 
  align-items: center;
}

.nav-item_img {
  width: 80rpx;
  height: 80rpx;
  margin: 36rpx 36rpx 36rpx 0;
}

.nav-item_rimg {
  flex: 1;
  display: flex;
  flex-direction: row-reverse;
  margin-right: 36rpx;
}

.nav-item_rimg image{
  background-size: cover;
  width: 36rpx;
  height: 36rpx;
}

.nav-item_text{
  display: flex;
  flex-direction: column;
  margin-left: 10rpx;
}


.nav-item_tname {
  font-size: 32rpx;
  font-weight:  bold;
  color: #39404d;
  margin-bottom: 24rpx;
}

.nav-item_bname {
  font-size: 28rpx;
  color: #999999;
}

.bottom-navigator{
  flex: 1 ;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-items: center;
  justify-content: space-around;
}

.botnav-item {
  width: 30%;
  height: 200rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}


.botnav-item_img {
  width: 64rpx;
  height: 64rpx;
  background-size: cover;
  margin: 40rpx 20rpx 0 20rpx;
}

.botnav-item_name{
  margin-top: 20rpx;
  font-size: 28rpx;
  font-weight: 600; 
  color: #39404d;
}

.filling{
  height: 20rpx;
  background-color: #f5f5f5;
}